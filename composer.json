{
  "name": "3xw/cakephp-pages",
  "description": "CakePHP pages plugins",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.5.9",
    "cakephp/cakephp": "^4.0",
    "3xw/cakephp-tinymce": "dev-next",
    "3xw/cakephp-attachment": "dev-next"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Trois\\Pages\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trois\\Pages\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
